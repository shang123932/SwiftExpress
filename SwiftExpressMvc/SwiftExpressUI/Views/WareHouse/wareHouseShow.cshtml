
@{
    ViewBag.Title = "wareHouseShow";
}
<h2>wareHouseShow</h2>

<table class="table">
    <thead>
        <tr>
            <td>仓库名称</td>
            <td>仓库容量</td>
            <td>仓库地址</td>
            <td>仓库备注</td>
            <td>仓库状态</td>
            <td>详情</td>
        </tr>
    </thead>
    <tbody id="body">
    </tbody>
</table>

<script src="~/Scripts/jquery-3.3.1.js"></script>
<script>
    $().ready(function () {
        $.ajax({
            url: "/WareHouse/wareHouseShow",
            dataType: "json",
            type: "POST",
            success: function (res) {
                var tbs = "";
                $(res.wareahouselist).each(function () {
                    var tb = "<tr>"
                   +"<td>" + this.WareHouseName + "</td> "
                   +"<td>" + this.WareHouseCapacity  + "</td> "
                   +"<td>" + this.WareHouseAddress   + "</td> "
                   +"<td>" + this.WareHouseRemark    + "</td> "
                   +"<td>" + this.WareHouseStatus + "</td> "
                   +" <td><a href='/WareHouse/wareHouseUpdate?pid=" + this.WareHouseId + "'></a>详情"
                   +"< a href = '/WareHouse/wareHouseDel?id=" + this.WareHouseId +"' > 删除</a ></td > "
                   +" </tr >";
                    tbs += tb;
                })
                $("#body").html(tbs);
            }
        })
    })
</script>
